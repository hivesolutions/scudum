#!/bin/bash
# -*- coding: utf-8 -*-

DISTRIB=$(cat /etc/scudum/DISTRIB)
VERSION=$(cat /etc/scudum/VERSION)
ROOTFS_VERSION=$(cat /etc/scudum/ROOTFS_VERSION)
KVERSION=$(uname -r)
SVERSION=$DISTRIB-$VERSION
EXTRAS=$(cat /etc/scudum/CONFIGURED)

set -e +h

echo -e "Scudum $SVERSION (rootfs $ROOTFS_VERSION) (GNU/Linux $KVERSION)"
echo $EXTRAS
